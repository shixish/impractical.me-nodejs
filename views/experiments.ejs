<% layout('content-layout') -%>
<div class="container">
  <% include section/header %>
  <% include section/breadcrumb %>
  
  <% for (var i in node.experiments){ %>
    <% if (i%2==0){ %>
      <div class="row">
    <% } %>
    <%
      teaser_url = node.experiments[i];
      teaser_node = nodes[teaser_url];
    %>
    <% include experiments/section/teaser %>
    
    <% if (i%2==1 || i==node.experiments.length-1){ %>
      </div><!-- end of row-->
    <% } %>
  <% } %>
</div>